{
  "include": ["src/**/*.ts"],
  "exclude": ["src/**/*.spec.ts", "src/**/*.test.ts", "src/**/*.d.ts", "dist/**/*", "node_modules/**/*"],
  "reporter": ["text", "text-summary", "html", "json-summary"],
  "report-dir": "coverage",
  "temp-dir": ".nyc_output",
  "all": true,
  "check-coverage": false,
  "lines": 80,
  "functions": 80,
  "branches": 80,
  "statements": 80,
  "require": ["ts-node/register"],
  "extension": [".ts"]
}
