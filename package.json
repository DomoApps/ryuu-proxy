{
  "name": "@domoinc/ryuu-proxy",
  "version": "4.1.0",
  "description": "a middleware that provides a proxy for local domo app development",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "tslint src/**/*.ts --project ./tsconfig.json",
    "test": "mocha && rm -rf test",
    "tdd": "mocha --watch",
    "prebuild": "npm run lint",
    "build": "rm -rf ./dist && tsc",
    "releaseAlpha": "npm run build && npm publish --tag alpha",
    "releaseBeta": "npm run build && npm publish --tag beta",
    "release": "npm run build && npm publish",
    "start": "npm run build -- -w",
    "clean": "rm -rf node_modules && yarn"
  },
  "author": "AppTeam6 <Squad.AppTeam6@domo.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/DomoApps/ryuu-proxy.git"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "keywords": [
    "domo",
    "domo apps"
  ],
  "devDependencies": {
    "@types/busboy": "^0.2.3",
    "@types/chai": "^4.2.15",
    "@types/configstore": "^4.0.0",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.35",
    "@types/sinon": "^9.0.11",
    "@types/tough-cookie": "^4.0.0",
    "chai": "^4.3.4",
    "mocha": "^8.3.2",
    "mock-req": "^0.2.0",
    "sinon": "^9.2.4",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-loader": "^3.5.4",
    "tsutils": "^3.21.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "axios-cookiejar-support": "^1.0.1",
    "busboy": "^0.3.1",
    "configstore": "^5.0.1",
    "core-js": "^3.9.1",
    "form-data": "^4.0.0",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.6",
    "ryuu-client": "4.1.0",
    "tough-cookie": "^4.0.0"
  },
  "peerDependencies": {
    "ryuu": "^4.1.0"
  }
}
